<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <EditTextPreference
        android:name="SIP Username"
        android:defaultValue=""
        android:key="namePref"
        android:summary="Username for your SIP Account"
        android:title="SIP Username" />
    <EditTextPreference
        android:name="SIP Domain"
        android:defaultValue=""
        android:key="domainPref"
        android:summary="Domain for your SIP Account"
        android:title="SIP Domain" />
    <EditTextPreference
        android:name="SIP Password"
        android:defaultValue=""
        android:key="passPref"
        android:password="true"
        android:summary="Password for your SIP Account"
        android:title="SIP Password" />
    <EditTextPreference
        android:name="SIP Proxy"
        android:defaultValue=""
        android:key="proxyPref"
        android:summary="SIP Proxy address to connect through"
        android:title="Proxy address" />

</PreferenceScreen>